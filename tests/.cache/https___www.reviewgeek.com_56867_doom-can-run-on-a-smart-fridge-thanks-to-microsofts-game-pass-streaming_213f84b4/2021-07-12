<script type="text/javascript">
	// Check for newsletter flag or already-signed-up flag first:
	if (!document.cookie.includes('htgreader') && !document.cookie.includes('htg_newsletter_subform_hide')) {
		var req = new XMLHttpRequest();
		req.addEventListener("loadend", function() {
			var v2adslist = JSON.parse(req.response);
			// This will be an array of ads - generally one in-page and one popup, but could be one or the other etc.
			if (v2adslist) {
				var insertAfter = function(newNode, existingNode) {
					existingNode.parentNode.insertBefore(newNode, existingNode.nextSibling);
				}

				v2adslist.forEach(function(v2data) {
					if (v2data.adtype == 'popup') {
						if (+v2data.popup_delay != v2data.popup_delay) {
							v2data.popup_delay = 60;
						}

						if (window.admiral_engaged) {
							// Add an extra 30 seconds
							v2data.popup_delay += 30;
						}

						// Enforce a minimum size for popups - both on mobile and desktop
						const vw = Math.max(document.documentElement.clientWidth || 0, window.innerWidth || 0)
						const vh = Math.max(document.documentElement.clientHeight || 0, window.innerHeight || 0)

						if (vw < 360 || vh < 640) {
							return;
						}

						window.showEmailPopupScrollWatcher = setInterval(function() {
							let scrollTop = window.scrollY;
							let docHeight = document.body.offsetHeight;
							let winHeight = window.innerHeight;
							let scrollPercent = scrollTop / (docHeight - winHeight);

							if (scrollPercent > 0.17) {
								clearInterval(window.showEmailPopupScrollWatcher);

								setTimeout(function() {
									var newDiv = document.createElement('div');
									var positionStyle = '';
									var backdrop = ''; // Note - no backdrop for corner styles

									if (/Android|webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
										v2data.popup_style = 'full-width';
										backdrop = '<div id="v2campaignpopup_backdrop" style="z-index: 100000101;"></div>';
									}

									if (v2data.popup_style == 'centered') {
										positionStyle = 'top: 50%; left: 50%; -ms-transform: translateX(-50%) translateY(-50%); -webkit-transform: translate(-50%,-50%); transform: translate(-50%,-50%);';
										backdrop = '<div id="v2campaignpopup_backdrop"></div>';
									} else if (v2data.popup_style == 'fadein') {
										positionStyle = 'top: 50%; left: 50%; opacity: 0; -ms-transform: translateX(-50%) translateY(-50%); -webkit-transform: translate(-50%,-50%); transform: translate(-50%,-50%); -webkit-animation: moka-fade-in 1s forwards; -webkit-animation-delay: 0s; animation: moka-fade-in 1s forwards; animation-delay: 0s;';
										backdrop = '<div id="v2campaignpopup_backdrop"></div>';

									} else if (v2data.popup_style == 'slideinside') {
										positionStyle = 'top: 50%; left: -100%; -webkit-animation: moka-slide-in 1s forwards; -webkit-animation-delay: 0s; animation: moka-slide-in 1s forwards; animation-delay: 0s;';
										backdrop = '<div id="v2campaignpopup_backdrop"></div>';

									} else if (v2data.popup_style == 'slideintop') {
										positionStyle = 'top: -100%; left: 50%; -webkit-animation: moka-slide-in 1s forwards; -webkit-animation-delay: 0s; animation: moka-slide-in 1s forwards; animation-delay: 0s;';
										backdrop = '<div id="v2campaignpopup_backdrop"></div>';

									} else if (v2data.popup_style == 'popuplr') {
										positionStyle = 'bottom: 5px; right: 5px;';
									} else if (v2data.popup_style == 'popupur') {
										positionStyle = 'top: 60px; right: 5px;';
									} else if (v2data.popup_style == 'popupll') {
										positionStyle = 'bottom: 5px; left: 5px;';
									} else if (v2data.popup_style == 'popupul') {
										positionStyle = 'top: 60px; left: 5px;';
									} else if (v2data.popup_style == 'full-width') {
										positionStyle = 'top: 50%; left: 0; width: 100%; -ms-transform: translateY(-50%); -webkit-transform: translateY(-50%); transform: translateY(-50%); box-shadow: none !important;';
									}

									window.closev2popup = function() {
										try { document.getElementById('v2campaignpopup').remove(); } catch {}
										try { document.getElementById('v2campaignpopup_backdrop').remove();  } catch {}
										try { document.cookie = 'htg_newsletter_subform_hide=1; expires=Sun, 1 Jan 2023 00:00:00 UTC; path=/'; } catch {}
									}

									if (/Android|webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
										// Make sure mobile header doesn't cover up the close button
										positionStyle += 'z-index: 100000102; ';
									} else {
										positionStyle += 'z-index: 100007; ';
									}

									var closeButton = '<label id="moka-v2campaign-closebtn" onclick="closev2popup()">&times;</label>';

									newDiv.innerHTML = backdrop + '<div id="v2campaignpopup" style="position: fixed; ' + positionStyle + ' overflow: auto; max-height: 100%;">' + closeButton + v2data.content + '</div>';
									
									// Last ditch check to see if we shouldn't do this (e.g., HTG native popup already went)
									if (!document.cookie.includes('htg_newsletter_subform_hide')) {
										document.body.append(newDiv); 

										// Now that we appended the content, set any hidden refer tag
										(function() {
											if (document.referrer) {
												var x = document.getElementsByName('moka-pagerefer');
												for (var i = 0; i < x.length; i++) {
													x[i].value = document.referrer;
												}
											}
										})();

										// Finally, log that we showed the popup
										var trackRequest = new XMLHttpRequest();
										trackRequest.open('GET', '/emv2/track?a=' + v2data.short_name);
										trackRequest.send();
									}
								}, v2data.popup_delay * 1000);
							}
						}, 500);
					} else if (v2data.adtype == 'midpage' || v2data.adtype == 'bottomofpage') {
						// Only do this on actual articles:
						var articleArea = document.getElementsByClassName('entry-content');
						if (articleArea && articleArea.length > 0) {
							articleArea = articleArea[0];
							var paragraphCandidates = articleArea.children;
							var paragraphs = [];
							// Locate the actual p's - and non-empty ones too
							for (var actualParag = 0; actualParag < paragraphCandidates.length; actualParag++) {
								var candidate = paragraphCandidates.item(actualParag);
								if (candidate && candidate.nodeName == 'P' && candidate.innerText != '') {
									paragraphs.push(candidate);
								}
							}

							if (paragraphs.length < v2data.inpage_paragno) {
								v2data.inpage_paragno = paragraphs.length - 1;
							}

							if (+v2data.inpage_minparagraphs <= paragraphs.length) {
								var newDiv = document.createElement('div');
								newDiv.innerHTML = '<div id="v2campaignmidpage">' + v2data.content + '</div>';						

								var insertDestCandidate = false;
								
								if (v2data.adtype == 'bottomofpage') {
									// We're counting from the bottom
									var fromBottom = paragraphs.length - v2data.inpage_paragno;
									if (fromBottom < 1) fromBottom = 1;
									insertDestCandidate = paragraphs[fromBottom];
								} else {
									// From the top!
									insertDestCandidate = paragraphs[v2data.inpage_paragno];
								}
								if (insertDestCandidate) insertAfter(newDiv, insertDestCandidate);
							}
						}
					}
				});
			}
		});
		var emv2_url = "/emv2/inpage?p=56867";
		if (/Android|webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
			emv2_url += '&d=mobile';
		} else {
			emv2_url += '&d=desktop';
		}
		try {
			const urlParams = new URLSearchParams(window.location.search);
			const htgForceAd = urlParams.get('htg_force_ad');
			if (htgForceAd && +htgForceAd == htgForceAd) {
				emv2_url += '&htg_force_ad=' + htgForceAd;
			}
		} catch (e) {
		}
		req.open("GET", emv2_url);
		req.send();
	}
</script>
