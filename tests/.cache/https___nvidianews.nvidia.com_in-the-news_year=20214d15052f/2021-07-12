        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        var ga_event = function(type,title,id){
        //console.log(type,title,id);
            try {
                    
                        if(type == "PublishDate")
                        {
                        ga('ipr.send', 'event', type, title, id,{
                          'nonInteraction': 1
                        });
                        }
                        else
                        {
                        ga('ipr.send', 'event', type, title, id);
                        }
                    
            }
            catch(e) {

            }
        }

        function ga_social_event(network,action,target){
                 ga('ipr.send', 'social', network, action, target);
            }

        ga('create', 'UA-65241159-1', {'name':'ipr','cookieName': 'ipr_ga'});

        

            ga('ipr.set', 'contentGroup4', '5432fab9c80467062000a6a8');

            

            

             

            
              ga_event('PublishDate','View','November 8, 2011');
            
            
            
            
            
            
            
        

        
        

        
            ga('ipr.set', 'contentGroup3', '|');
        

        

        var referrer = document.referrer;
        if(referrer != "" && referrer.indexOf('?q') >=0){
          var qs = referrer.split('?q=')[1] || '';
          var final_qs = qs.split('&')[0]
          
            ga_event('Search',final_qs,'/in-the-news');
            //alert("send");
          
        }

        jQuery( document ).ready(function(){

            if(jQuery(".ipr_ga_mixed_gallery_page_flag").length > 0)
                ga('ipr.set', 'contentGroup2', 'mixed_gallery');

            ga('ipr.send', 'pageview');

            try {
                jQuery(".mixed_gallery_low_res_download, .mixed_gallery_hi_res_download").click(function(){
                    ga_event('mixed_gallery_download',jQuery(this).attr('title'),jQuery(this).attr('data-ipr-ga'));
                });
            }
            catch(e){

            }

            try {
                jQuery(".ga-fileview").click(function(){
                  page_id = "5432fab9c80467062000a6a8";
                  user_id = "";
                  if(user_id == ""){user_id='ANONYMOUS'}
                  track_event_label = page_id+"_"+user_id;
                  ga_event('fileView',jQuery(this).attr('data-ipr-ga'),track_event_label);
                });
            }
            catch(e){

            }

         
            if(typeof(addthis)!='undefined'){

              addthis.addEventListener('addthis.menu.share', function (evt) {
              ga_social_event( evt.data.service, 'Share', evt.data.url);
              });
            }

            jQuery('a').click(function(){
                if(jQuery(this).attr("href") != ''){
                    ga_event('linkClick',jQuery(this).attr("href"),jQuery(this).text());
                }

            })
        });

        
        // For Tracking Whole Clients Pageviews

         ga('create', 'UA-111728443-1', 'auto','clientTracker');

         // Testing - UA-111727747-1 Test local account
         // Production - 'UA-111728443-1' 

         // custom dimension name is : Site
         ga('clientTracker.set', 'dimension1', 'nvidia');
         ga('clientTracker.send', 'pageview');

         var audio_view = function(file_id){
              page_id = "5432fab9c80467062000a6a8";
              user_id = "";
              if(user_id == ""){user_id='ANONYMOUS'}
              track_event_label = page_id+"_"+user_id;
              ga_event('fileView',file_id,track_event_label);
              }


        var file_view = function(file_id){
              page_id = "5432fab9c80467062000a6a8";
              user_id = "";
              if(user_id == "")
              {
                
                   user_id='ANONYMOUS'
                
              }
              track_event_label = page_id+"_"+user_id;
