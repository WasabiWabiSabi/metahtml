<script>
	/* Wait for ccpa bar to load. Hide it after interacting */
	var load = function () {
		let ccpa = document.getElementsByClassName('fc-ccpa-root')[0];
		
		if (ccpa) {
			if (sessionStorage.getItem("ccpaInteracted") === '1') {
				ccpa.setAttribute('style', 'left:-100% !important');
			}
			else {
				ccpa.addEventListener('click', function(e) {
					ccpa.setAttribute('style', 'left:-100% !important');
					sessionStorage.setItem("ccpaInteracted", "1");
				});
			}
		}

	}
	
	waitForElementToDisplay(".fc-ccpa-root", load, 1000, 9000);
	
	function waitForElementToDisplay(selector, callback, checkFrequencyInMs, timeoutInMs) {
		var startTimeInMs = Date.now();
		(function loopSearch() {
			if (document.querySelector(selector) != null) {
				callback();
				return;
			}
			else {
				setTimeout(function () {
					if (timeoutInMs && Date.now() - startTimeInMs > timeoutInMs)
						return;
					
					loopSearch();
				}, checkFrequencyInMs);
			}
		})();
	}
</script>
 
	




	
	
	

