function base64decode(str) {
var result = '';
for (var i = 0, _a; i < str.length; i++) {
_a = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789-_'.indexOf(str[i])
var strBits = _a.toString(2) || '';
result += '0'.repeat(6 - strBits.length) + strBits;
}
return result;
}
function getCMPId(encodedTCString) {
var bitField = base64decode(encodedTCString);
var bitsBeforeCmpId = 78;
var cmpIdBits = 12;
var bits = bitField.substr(bitsBeforeCmpId, cmpIdBits);
return parseInt(bits, 2);
}
var consentFromLocalStorage = window.localStorage.getItem('euconsent-v2');
if (consentFromLocalStorage && getCMPId(consentFromLocalStorage) !== 10) {
window.localStorage.removeItem('euconsent-v2');
}
}
