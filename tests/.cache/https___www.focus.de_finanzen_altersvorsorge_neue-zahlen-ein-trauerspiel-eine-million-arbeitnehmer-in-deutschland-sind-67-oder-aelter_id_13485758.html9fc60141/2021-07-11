                            var type = (eventType) ? eventType : 'std',
                                eventgroup = (eventName) ? eventName : 'event',
                                defaultEvent = {
                                    'event': eventgroup,
                                    'eventCategory': category,
                                    'eventAction': action,
                                    'eventLabel': label
                                };
                            
                            if (category.toLowerCase() == 'social') {
                                defaultEvent['eventSocialNetwork'] = network;
                                dataLayer.push(defaultEvent);
                                dataLayer.push({'eventSocialNetwork': void 0});  
                            } else if (type == 'nonInteraction') {
                                defaultEvent['eventNonInteraction'] = 1;
                                dataLayer.push(defaultEvent);
                                dataLayer.push({'eventNonInteraction': void 0});
                            } else if ( typeof(oCustomVarsIn) == "object" ) {
                                var oCustomVars = {};
                                for (var param in oCustomVarsIn) {
                                    oCustomVars[param] = oCustomVarsIn[param];
                                }
                                if ( type == "std" ) {
                                    for (var param in defaultEvent) {
                                        oCustomVars[param] = defaultEvent[param];
                                    }
                                }                   
                                dataLayer.push(oCustomVars);                    
                                for (var param in oCustomVars) {
                                    oCustomVars[param] = void 0;
                                }                   
                                dataLayer.push(oCustomVars);                    
                            } else {
                                dataLayer.push(defaultEvent);
                            }
                        }
                        function FOL_GTM_PushSiteSearch(searchterm,category) { /*dataLayer.push({'event': 'search','siteSearchTerm': searchterm,'siteSearchCategory': category});*/ }
                        function FOL_GTM_VirtPageview(category,agof) {
                            var pageCategory = (category) ? category : "artikel";
                            dataLayer.push({
                                'event': 'virtualPage',
                                'pageCategory': pageCategory,
                                'pageIVW_AGOF': __fol_iam_data.st + '|' + __fol_iam_data.cp+'|P0000|J0001|D0000',
                                'virtualPage': 'true',
                                'virtualPagePath': '13485758_/finanzen/altersvorsorge/neue-zahlen-ein-trauerspiel-eine-million-arbeitnehmer-in-deutschla_' + category 
                                });
                        }
                        function getCookie(name) {
                            var value = "; " + document.cookie;
                            var parts = value.split("; " + name + "=");
                            if (parts.length == 2) return parts.pop().split(";").shift();
                        };
                        function setCookie(name, value, days) {
                            days = days || 0;
                            if(days > 0){
                                var d = new Date;
                                d.setTime(d.getTime() + 24*60*60*1000*days);
                            }
                            document.cookie = name + "=" + value + ";path=/;"+((days > 0)?"expires=" + d.toGMTString():"");
                        };
                        function deleteCookie(name) { setCookie(name, '', -1); };
                        
                        function FOL_GTM_InitialDataLayer() {
                                if ( typeof getCookie("fol.breakingnews") !== 'undefined' && getCookie("fol.breakingnews") != "" && document.location.pathname.indexOf(getCookie("fol.breakingnews")) >= 0 ) {
                           dataLayer.push({'pageBreaking': 'true'});
                           setCookie("fol.breakingnews","");
                        } else {
                           dataLayer.push({'pageBreaking': 'false'});
                        }
                                    if(typeof getCookie("fol.HomeLNS") !== "undefined" && getCookie("fol.HomeLNS") !== "" && document.location.pathname.indexOf(getCookie("fol.HomeLNS")) >= 0){
                        dataLayer.push({'localHomeIntegration': 'true'});
                        deleteCookie('fol.HomeLNS');
                    }else{
                        dataLayer.push({'localHomeIntegration':'false'});
                    }
                                    var FOLGTMLoginState='false';try { FOLGTMLoginState= (typeof getCookie("clCheck") !== "undefined" && getCookie("clCheck") == "true") ? 'true' : FOLGTMLoginState; } catch(oError) {};
                            dataLayer.push({'visitorLoginState': FOLGTMLoginState});
                                    try { if ( TFT_Ext_FOL_getUrlParam(document.location,"omcid") != "" ) {
                                FOL_GTM_PushEvent('cmsRedirect','redirect',unescape(TFT_Ext_FOL_getUrlParam(document.location,"omcid")),'','nonInteraction');
                            }} catch(oError) {/*ignore*/}
                                     
                        }
                        dataLayer = typeof dataLayer === 'undefined' ? [] : dataLayer; dataLayer.push({'pageArea': 'focus',
                                          'pageType': 'desktop',
