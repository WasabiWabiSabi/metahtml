	function fb_t_convert() {
		var url = 'https://www.howtogeek.com/public/trackBBClick.php?s=reviewgeek';

		var htgm = document.cookie.match('htg=([0-9]*)');
		if (htgm && htgm.length == 2) {
			url += '&htgm=' + htgm[1];
		}
		var fbpm = document.cookie.match('_fbp=([0-9a-zA-Z\.]*)');
		if (fbpm && fbpm.length == 2) {
			url += '&fbp=' + fbpm[1];
		}
		var fbcm = document.cookie.match('_fbc=([0-9a-zA-Z\.]*)');
		if (fbcm && fbcm.length == 2) {
			url += '&fbc=' + fbcm[1];
		}

		url += '&r=' + Date.now();

		var xhr = new XMLHttpRequest();
		xhr.open('GET', url);
		xhr.send()
	}
</script>
