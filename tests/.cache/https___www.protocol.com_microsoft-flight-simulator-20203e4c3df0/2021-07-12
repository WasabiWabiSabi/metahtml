function remove_col_with_empty_post(){
  document.querySelectorAll(".flex-row-a .col .mb-2:empty").forEach(function(item){
  	item.parentElement.parentElement.removeChild(item.parentElement);
  })
}
  remove_col_with_empty_post();
</script><!-- End User Code --><!-- User Code --><script>
function isElementInViewport(el, extraOffsetTop, extraOffsetBottom) {
        var rectangle = el.getBoundingClientRect(),
            isHidden = rectangle.x === 0 && rectangle.y === 0 && rectangle.height === 0 && rectangle.top === 0;

        if (isHidden) {
            return false;
        }

        if (!extraOffsetTop) {
            extraOffsetTop = 0;
        }
        if (!extraOffsetBottom) {
            extraOffsetBottom = 0;
        }

        var extraTop = rectangle.top - extraOffsetTop,
            extraBottom = rectangle.bottom + extraOffsetBottom,
            windowHeight = document.documentElement.clientHeight,
            topVisible = extraTop > 0 && extraTop < windowHeight,
            bottomVisible = extraBottom < windowHeight && extraBottom > 0,
            overallVisible = extraTop < 0 && extraBottom > windowHeight;

        return topVisible || bottomVisible || overallVisible;
    }
  function Ithrottle (callback, limit) {
      var wait = false;
      return function () {
        if (!wait) {

          callback.apply(null, arguments);
          wait = true;
          setTimeout(function () {
            wait = false;
          }, limit);
        }
      }
    }
</script><!-- End User Code --><!-- User Code --><script>
document.querySelectorAll('.newsletter-element__form input[type="checkbox"]').forEach(function (item) {
	item.checked = true;
});
var pc_pc = document.querySelector(".widget__body .photo-caption + .photo-credit");
var pc = document.querySelector(".widget__body .photo-caption ");
if (pc_pc) {
	if (pc.querySelector("p")) {
		pc.querySelector("p").innerHTML += " | " + pc_pc.innerHTML;
	} else {
		pc.innerHTML += " | " + pc_pc.innerHTML;
	}
	pc_pc.innerHTML = "";
}
if (document.querySelector(".article__splash-custom .post-author-list") && document.querySelectorAll(".article__splash-custom .post-author-list .post-author").length > 1) {
	document.querySelector(".article__splash-custom .post-author-list").className = "post-author-list multi"
}
 
