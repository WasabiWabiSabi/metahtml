window.jQueryQueueTimer = setInterval(() => {
if (window.jQueryQueue && window.jQuery) {
// Limpa o interval
clearInterval(window.jQueryQueueTimer)
// Vai processar depois do jQuery
let fn = null
while (fn = window.jQueryQueue.shift()) {
if ('function' == typeof fn)
fn(jQuery)
}
}
}, 100)
