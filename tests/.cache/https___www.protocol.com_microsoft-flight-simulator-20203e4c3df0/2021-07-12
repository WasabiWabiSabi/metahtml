    addSize([1024, 0], [[728, 90]]).
		addSize([768, 0], [[728, 90], [300,250],[468, 60]]).
		addSize([0, 0], [[320, 50],[300, 250],[300,600]]).
		build();
	righttRailMapping = googletag.sizeMapping().
		addSize([768, 0], [[300, 600], [300,250]]).
		addSize([0, 0], []).
		build();
    
    postsmodulemapping = googletag.sizeMapping().
		addSize([768, 0],  [300,250]).
		addSize([0, 0], [300,250]).
		build();
    googletag.defineSlot(`/21901267728/${ad_modifier}`, [[468, 60], [728, 90], [300, 600], [970, 250], [300, 250]], 'banner-top').defineSizeMapping(bannerMapping).addService(googletag.pubads());

