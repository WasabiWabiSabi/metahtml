                        require(['extNoAmd.ioamTracking', 'configuration'], function (mod, conf) {
                            if (window.ippenIoamCalled !== true &&
                                window.iom &&
                                    conf.page.ivwTrackingInfo.isTrackingDisabled !== true) {
                                if (window.iom.h &&
                                    conf.features.ivw_h_method === true &&
                                        conf.page.deviceType === 'MOBILE') {
                                    window.iom.h({"st": conf.page.ivwTrackingInfo.st, "cp": conf.page.ivwTrackingInfo.cp, "sv": "ke", "sc": "yes"}, 2);
                                    window.ippenIoamCalled = true;
                                } else if (window.iom.c) {
                                    window.iom.c({"st": conf.page.ivwTrackingInfo.st, "cp": conf.page.ivwTrackingInfo.cp, "sv": "ke", "sc": "yes"}, 2);
                                    window.ippenIoamCalled = true;
                                }
                            }
                        }, function () {
                            if (window.ippenIoamCalled !== true) {
                                ippenErr.push && ippenErr.push(["!!!!! IVW TRACKING FAILED !!!!!", "https://script.ioam.de/iam", "", "", ""]);
                            }
                        });

                        
                        require(['modules/id.tm'], function (trackingManager) {
                            var tm = new trackingManager;
                            tm.init();
                        });

                        
                        if (false) {
                            require(['modules/id.commercial'], function (Commercial) {
                                new Commercial().init();
                            });
                        }

                        
                        if (false) {
                            require(['modules/id.linkpulsePaywallLoader'], function (LinkpulsePaywallLoader) {
                                var linkpulse = new LinkpulsePaywallLoader();
                                linkpulse.conf.linkpulseScriptUrl = "";
                                linkpulse.conf.productOnlineComplete = "";
                                linkpulse.init();
                            });
                        }

                        require(['vendor/vendor-build', 'modules/global-modules-build'], function () {
                            require(['modules/id.core'], function (core) { 
                                if (document.readyState === "interactive") {
                                    core.initComponents();
                                } else { 
                                    require(['vendor/requirejs/domReady!'], function () {
                                        core.initComponents();
                                    });
                                }
                            });
                        });
                    }
                };
                document.getElementsByTagName('head')[0].appendChild(s);
            }());
        }
        </script>
